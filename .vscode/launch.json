    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Telegram Bot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\integrations\\telegram.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "envFile": "${workspaceFolder}/.env"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Create Assets",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\create.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}